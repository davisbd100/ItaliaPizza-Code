                    <Button x:Name="CancelarButtonInicioSesion" Content="CANCELAR" HorizontalAlignment="Left" Height="37" Margin="190,271,0,0" VerticalAlignment="Top" Width="91" Background="#FFE53B28" BorderBrush="White" Foreground="White" FontFamily="Calibri Light" FontSize="16" Click="CancelarButton_Click"/>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>