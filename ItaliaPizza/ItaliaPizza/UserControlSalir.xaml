<UserControl x:Class="ItaliaPizza.UserControlSalir"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ItaliaPizza"
             mc:Ignorable="d" 
             d:DesignHeight="86" Width="105.667">
    <Grid Margin="0,0,10.333,0">
        <Button x:Name="SalirButton" HorizontalAlignment="Left" Height="86" VerticalAlignment="Top" Width="103" BorderBrush="{x:Null}" Click="SalirButton_Click" Background="Transparent">
            <StackPanel>
                <Image Source="Recursos/Salir.png" Stretch="Uniform" Height="76" Width="87"/>
            </StackPanel>
        </Button>
    </Grid>
</UserControl>
